    "elif n == 1: \n",
    "    print(b) \n",
    "else:  \n",
    "    print(a,end=\" \")\n",
    "    print(b,end=\" \")\n",
    "    for i in range(2,n): \n",
    "        c = a + b\n",
    "        print(c,end=\" \")\n",
    "        a = b \n",

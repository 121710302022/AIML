    "### **3. Roots of quadratic equation**\n",
    "### Take the coefficients a,b,c from the user"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "a=3\n",
      "b=8\n",
      "c=1\n",
      "Root 1= -8.0\n",
      "Root 2= -8.0\n"
     ]
    }
   ],
   "source": [
    "a=int(input(\"a=\"))\n",
    "b=int(input(\"b=\"))\n",
    "c=int(input(\"c=\"))\n",
    "d=b*b-4*a*c\n",
    "srt=d**(1/2)\n",
    "s=round(srt.imag,5)\n",
    "if d>0:\n",
    "    print(\"Root 1=\",(-b+s/(2*a)))\n",
    "    print(\"Root 2=\",(-b-s/(2*a)))\n",
    "elif d==0:\n",
    "    print(\"Root 1=\",-b/(2*a),\"= Root 2\")\n",
    "else:\n",
    "    print(\"Root 1=\",end=\"\")\n",
    "    print(-b/(2*a),'+i',s)\n",
    "    print(\"Root 2=\",end=\"\")\n",
    "    print(-b/(2*a),'-i',s)\n",
    "    "
   ]
  },
